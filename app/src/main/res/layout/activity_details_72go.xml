<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Details_72go">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="1520dp"
        android:layout_height="1900dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/green_gradient" />

    <Button
        android:id="@+id/button"
        android:layout_width="210dp"
        android:layout_height="61dp"
        android:layout_marginTop="1197dp"
        android:layout_marginEnd="645dp"
        android:layout_marginRight="645dp"
        android:text="RETURN"
        android:textAllCaps="true"
        android:textColor="#040303"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="265dp"
        android:layout_height="34dp"
        android:layout_marginTop="624dp"
        android:layout_marginEnd="628dp"
        android:layout_marginRight="628dp"
        android:text="What does 72 go mean?"
        android:textAllCaps="true"
        android:textColor="#0C0C0C"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="720dp"
        android:layout_marginEnd="580dp"
        android:layout_marginRight="580dp"
        android:text="72 go means that you can rent a care for at 7 days."
        android:textAllCaps="false"
        android:textColor="#1B1A1A"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="748dp"
        android:layout_marginEnd="616dp"
        android:layout_marginRight="616dp"
        android:text="Price/km :10 euro or for an entire day 60 euro"
        android:textColor="#101010"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11" />
</androidx.constraintlayout.widget.ConstraintLayout>