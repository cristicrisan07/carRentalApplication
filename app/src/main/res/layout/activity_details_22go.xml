<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Details_22go">

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="900dp"
        android:layout_height="1568dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/green_gradient" />

    <Button
        android:id="@+id/return_button"
        android:layout_width="210dp"
        android:layout_height="61dp"
        android:layout_marginTop="994dp"
        android:layout_marginEnd="346dp"
        android:layout_marginRight="346dp"
        android:text="RETURN"
        android:textAllCaps="true"
        android:textColor="#0B0B0B"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView12"
        app:layout_constraintTop_toTopOf="@+id/imageView12" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="508dp"
        android:layout_marginEnd="340dp"
        android:layout_marginRight="340dp"
        android:text="What does 22 go mean?"
        android:textAllCaps="true"
        android:textColor="#1B1A1A"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView12"
        app:layout_constraintTop_toTopOf="@+id/imageView12" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="310dp"
        android:layout_height="45dp"
        android:layout_marginTop="600dp"
        android:layout_marginEnd="300dp"
        android:layout_marginRight="300dp"
        android:text="22 go means that you can rent a care for at 2 days."
        android:textColor="#050505"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView12"
        app:layout_constraintTop_toTopOf="@+id/imageView12" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="302dp"
        android:layout_height="39dp"
        android:layout_marginTop="644dp"
        android:layout_marginEnd="308dp"
        android:layout_marginRight="308dp"
        android:text="Price/km :10 euro or for an entire day 40 euro"
        android:textColor="#0E0E0E"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView12"
        app:layout_constraintTop_toTopOf="@+id/imageView12" />
</androidx.constraintlayout.widget.ConstraintLayout>